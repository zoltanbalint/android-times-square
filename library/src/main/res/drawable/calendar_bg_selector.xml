<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:state_range_first="true" android:state_selected="true">
        <bitmap android:src="@drawable/diagonal1" />
    </item>
    <item app:state_range_last="true" android:state_selected="true">
        <bitmap android:src="@drawable/diagonal2" />
    </item>
    <item app:state_range_middle="true">
        <color android:color="@color/calendar_selected_range_bg" />
    </item>
    <item android:state_selected="true">
        <color android:color="@color/calendar_selected_day_bg" />
    </item>
    <item android:state_pressed="true">
        <color android:color="@color/calendar_selected_day_bg" />
    </item>
    <item app:state_current_month="false">
        <color android:color="@color/calendar_inactive_month_bg" />
    </item>
    <item app:state_highlightable="true" app:state_selectable="false">
        <color android:color="@color/calendar_highlightable_day_bg" />
    </item>
    <item app:state_highlightable="false" app:state_selectable="false">
        <color android:color="@color/calendar_unhighlightable_day_bg" />
    </item>
    <item app:state_highlighted="true">
        <color android:color="@color/calendar_highlighted_day_bg" />
    </item>
    <item app:state_today="true">
        <color android:color="@color/calendar_bg_today" />
    </item>
    <item>
        <color android:color="@color/calendar_active_month_bg" />
    </item>
</selector>
